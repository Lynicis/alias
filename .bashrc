# Terminal Aliases by Lynicis
# General
alias godmode="sudo su"
alias offG="exit"
alias saupd="sudo apt update"
alias saupg="sudo apt upgrade="
alias rf="sudo rm -r"

# Project
# for yarn
alias babe="yarn add"
alias upBabe="yarn run start"
alias buildBabe="yarn run build"
alias devBabe="yarn run dev"

